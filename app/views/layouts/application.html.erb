<!DOCTYPE html>
<html>
<head>
  <title>Forum</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container">
	<% if user_signed_in? %>
  <%= link_to('Logout', destroy_user_session_path, method: :delete, class: 'navbar-brand pull-right btn btn-warning') %>        
  <%end%>
<%= yield %>
</div>

</body>
</html>
